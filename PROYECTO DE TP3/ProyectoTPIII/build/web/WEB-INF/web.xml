<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>SA</servlet-name>
        <servlet-class>local.servlets.SA</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SA</servlet-name>
        <url-pattern>/SA</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>S1</servlet-name>
        <servlet-class>local.servlets.S1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>S1</servlet-name>
        <url-pattern>/S1</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>S2</servlet-name>
        <servlet-class>local.servlets.S2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>S2</servlet-name>
        <url-pattern>/S2</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>S3</servlet-name>
        <servlet-class>local.servlets.S3</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>S3</servlet-name>
        <url-pattern>/S3</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            3000
        </session-timeout>
    </session-config>
    
    <welcome-file-list>
        <welcome-file>v_S1.jsp</welcome-file>
    </welcome-file-list>
    
    <context-param>
        <description>Primer Carrito de Compras</description>
        <param-name>carrito</param-name>
        <param-value>1</param-value>
    </context-param>
    
</web-app>
